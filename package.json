{
  "name": "uni-pushy-server",
  "version": "0.1.0",
  "build": 101,
  "description": "A uni-app code push server.",
  "author": "SunSeekerX",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "yarn build && docker build -t upushy .",
    "build:docker-server": "docker build -f Dockerfile.server -t upushy-server .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "serve": "cross-env NODE_ENV=development nest start --watch",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:lint-staged": "lint-staged",
    "gc": "git add -A && git-cz && git pull && git push",
    "up": "yarn upgrade-interactive",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "ali-oss": "^6.16.0",
    "argon2": "^0.28.2",
    "axios": "^0.21.4",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "express-useragent": "^1.0.15",
    "helmet": "^4.6.0",
    "iconv-lite": "^0.6.3",
    "internal-ip": "^6.2.0",
    "is_js": "^0.9.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mysql": "^2.18.1",
    "node-disk-info": "^1.3.0",
    "node-rsa": "^1.1.1",
    "public-ip": "^4.0.4",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@types/cache-manager": "^3.4.2",
    "@types/express-useragent": "^1.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/md5": "^2.3.1",
    "@types/node": "^16.9.1",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}
