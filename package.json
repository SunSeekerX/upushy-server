{
  "name": "upushy-server",
  "version": "0.6.0",
  "private": true,
  "description": "A uni-app code push server.",
  "license": "MIT",
  "author": "SunSeekerX",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "yarn build && docker build -t upushy .",
    "build:docker-server": "docker build -f Dockerfile.server -t upushy-server .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gc": "git add -A && git-cz && git pull && git push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "serve": "cross-env NODE_ENV=development nest start --watch",
    "start": "nest start",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "up": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@nestjs/core": "^9.3.9",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "ali-oss": "^6.17.1",
    "argon2": "^0.30.3",
    "axios": "^1.3.4",
    "cache-manager": "^5.1.7",
    "cache-manager-redis-store": "^3.0.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "express-useragent": "^1.0.15",
    "helmet": "^6.0.1",
    "iconv-lite": "^0.6.3",
    "internal-ip": "6.2.0",
    "is_js": "^0.9.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mysql2": "^3.1.2",
    "node-disk-info": "^1.3.0",
    "node-rsa": "^1.1.1",
    "public-ip": "^4.0.4",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "svg-captcha": "^1.4.0",
    "svg64": "^2.0.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@types/cache-manager": "^4.0.2",
    "@types/express-useragent": "^1.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/md5": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.2",
    "@types/node-rsa": "^1.1.1",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.4.1"
}
