{
  "name": "upushy-server",
  "version": "0.3.0",
  "private": true,
  "description": "A uni-app code push server.",
  "license": "MIT",
  "author": "SunSeekerX",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "yarn build && docker build -t upushy .",
    "build:docker-server": "docker build -f Dockerfile.server -t upushy-server .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gc": "git add -A && git-cz && git pull && git push",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "serve": "cross-env NODE_ENV=development nest start --watch",
    "start": "nest start",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "up": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.6",
    "@nestjs/core": "^9.0.7",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.7",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.0",
    "ali-oss": "^6.17.1",
    "argon2": "^0.28.7",
    "axios": "^0.27.2",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "express-useragent": "^1.0.15",
    "helmet": "^5.1.1",
    "iconv-lite": "^0.6.3",
    "internal-ip": "6.2.0",
    "is_js": "^0.9.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mysql2": "^2.3.3",
    "node-disk-info": "^1.3.0",
    "node-rsa": "^1.1.1",
    "public-ip": "^4.0.4",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@types/cache-manager": "^4.0.1",
    "@types/express-useragent": "^1.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.6.2",
    "@types/node-rsa": "^1.1.1",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.0"
}
